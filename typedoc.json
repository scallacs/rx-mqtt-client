{
    "mode": "modules",
    "out": "docs/reference",
    "exclude": [
        "**/*.spec.ts"
    ],
    "theme": "default",
    "ignoreCompilerErrors": false,
    "excludePrivate": true,
    "excludeNotExported": true,
    "target": "ES5",
    "moduleResolution": "node",
    "preserveConstEnums": "true",
    "stripInternal": "true",
    "suppressExcessPropertyErrors": "true",
    "suppressImplicitAnyIndexErrors": "true",
    "module": "commonjs"
  }